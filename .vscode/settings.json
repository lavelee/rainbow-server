{
  "editor.formatOnSave": false,
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "dist": true,
    ".git": true,
    "node_modules": true,
    "coverage": true,
    ".config*.json": true,
    "package-lock.json": true,
  }
}